<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingButton">
        <attr name="completeColor" format="color" />
        <attr name="loadingColor" format="color" />
        <attr name="state" format="enum" >
            <enum name="Clicked" value="0"/>
            <enum name="Loading" value="1"/>
            <enum name="Completed" value="2"/>
        </attr>
    </declare-styleable>
</resources>